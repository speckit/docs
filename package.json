{
  "name": "SpecKit-documentation",
  "version": "1.0.0",
  "description": "Spec-driven development documentation website built with Mintlify",
  "main": "index.js",
  "scripts": {
    "dev": "mintlify dev",
    "build": "mintlify build",
    "validate": "npm run validate:content && npm run validate:seo",
    "validate:content": "node scripts/validate-content.js",
    "validate:seo": "node scripts/validate-seo.js",
    "sync:submodule": "./scripts/sync-submodule.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "test": "jest",
    "prepare": "husky install"
  },
  "keywords": [
    "SpecKit",
    "spec-driven-development",
    "documentation",
    "mintlify",
    "automation"
  ],
  "author": "SpecKit Team",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@mintlify/cli": "^4.0.0",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SpecKit/documentation.git"
  },
  "bugs": {
    "url": "https://github.com/SpecKit/documentation/issues"
  },
  "homepage": "https://SpecKit.dev"
}
